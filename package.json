{
  "name": "@gitevents/discord-bot",
  "version": "1.0.0",
  "type": "module",
  "description": "Create Events on Discord",
  "keywords": [
    "gitevents",
    "events",
    "issue ops",
    "discord"
  ],
  "homepage": "https://gitevents.org/",
  "bugs": {
    "url": "https://github.com/gitevents/discord-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitevents/discord-bot.git"
  },
  "license": "MIT",
  "author": "Patrick Heneise <patrick@zentered.co> (https://zentered.co)",
  "contributors": [
    {
      "name": "Patrick Heneise",
      "url": "https://zentered.co",
      "author": true
    }
  ],
  "main": "index.js",
  "scripts": {
    "start": "wrangler dev",
    "deploy": "wrangler publish",
    "postinstall": "husky install",
    "license-checker": "license-checker --production --onlyAllow=\"MIT;ISC;BSD-3-Clause;BSD-2-Clause;Apache-2.0\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{js}": [
      "eslint --cache --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@cfworker/jwt": "^4.0.2",
    "@zentered/issue-forms-body-parser": "^1.4.4",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "commitlint": "^17.0.2",
    "eslint": "^8.18.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "license-checker": "^25.0.1",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "wrangler": "^2.0.15"
  }
}
